{
    "name": "mrchejsonviewer",
    "displayName": "jsonviewer",
    "description": "Json Tree View on VSCode",
    "version": "1.0.0",
    "publisher": "ccimage",
    "repository":"github:ccimage/jsonviewer",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Formatters"
    ],
    "activationEvents": [
        "onCommand:extension.viewJson"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.viewJson",
                "title": "View json readable"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
